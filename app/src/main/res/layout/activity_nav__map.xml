<?xml version="1.0" encoding="utf-8"?>
<android.support.design.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.hp.travelkonnect.nav_Map"
    android:id="@+id/l1">

    <android.support.design.widget.AppBarLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:theme="@style/AppTheme.AppBarOverlay">

        <android.support.v7.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:background="?attr/colorPrimary"
            app:popupTheme="@style/AppTheme.PopupOverlay" />

    </android.support.design.widget.AppBarLayout>

    <include layout="@layout/content_nav__map"
        android:id="@+id/include" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:layout_margin="@dimen/fab_margin"
        app:srcCompat="@android:drawable/ic_dialog_email" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        app:srcCompat="@drawable/raj_map" />

    <ImageView
        android:id="@+id/imageViewJaisalmer"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:translationX="50dp"
        android:translationY="250dp"
        app:layout_anchor="@+id/imageView2"
        app:srcCompat="@drawable/pin" />
    <ImageView
        android:id="@+id/imageViewJaipur"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:translationX="240dp"
        android:translationY="260dp"
        app:layout_anchor="@+id/imageView2"
        app:srcCompat="@drawable/pin" />

    <ImageView
        android:id="@+id/imageViewBikaner"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:translationX="130dp"
        android:translationY="210dp"
        app:layout_anchor="@+id/imageView2"
        app:srcCompat="@drawable/pin" />
    <ImageView
        android:id="@+id/imageViewAjmer"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:translationX="200dp"
        android:translationY="290dp"
        app:layout_anchor="@+id/imageView2"
        app:srcCompat="@drawable/pin" />
    <ImageView
        android:id="@+id/imageViewMountAbu"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:translationX="120dp"
        android:translationY="350dp"
        app:layout_anchor="@+id/imageView2"
        app:srcCompat="@drawable/pin" />
    <ImageView
        android:id="@+id/imageViewUdaipur"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:translationX="160dp"
        android:translationY="370dp"
        app:layout_anchor="@+id/imageView2"
        app:srcCompat="@drawable/pin" />
    <ImageView
        android:id="@+id/imageViewMithdi"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:translationX="190dp"
        android:translationY="210dp"
        app:layout_anchor="@+id/imageView2"
        app:srcCompat="@drawable/pin" />

</android.support.design.widget.CoordinatorLayout>
